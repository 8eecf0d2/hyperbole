{
	"name": "jagwah-todo-site",
	"version": "0.0.0",
	"description": "Todo site using jagwah",
	"main": "dist/bundle.js",
	"scripts": {
		"watch": "NODE_ENV=development MODE=development webpack --watch --info-verbosity=verbose",
		"build:dev": "NODE_ENV=development MODE=production webpack",
		"build": "NODE_ENV=production MODE=production webpack",
		"server:dev": "NODE_ENV=development MODE=production webpack-serve --config ./webpack.config.js",
		"server": "NODE_ENV=production MODE=production webpack-serve --config ./webpack.config.js"
	},
	"author": "8eecf0d2",
	"license": "ISC",
	"devDependencies": {
		"connect-history-api-fallback": "^1.5.0",
		"koa-connect": "^2.0.1",
		"ts-loader": "^4.3.0",
		"typescript": "^2.8.3",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"webpack": "^4.8.1",
		"webpack-cli": "^2.1.3",
		"webpack-serve": "^1.0.4"
	},
	"dependencies": {
		"jagwah": "^0.0.9"
	}
}
